<div class="col-md-4 agileinfo_new_products_grid agileinfo_new_products_grid_dresses">
  <div class="agile_ecommerce_tab_left dresses_grid">
    <div class="hs-wrapper hs-wrapper2">
      <div class="product-images">
      <% product.food_images.each do |image| %>
        <%= image_tag( image.picture.url, alt: " ", class: "img-responsive" ) %>
      <% end %>
      </div>
      <div class="w3_hs_bottom w3_hs_bottom_sub1">
        <ul>
          <li>
            <a href="#" data-toggle="modal" data-target="#myModal9"><span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></a>
          </li>
        </ul>
      </div>
    </div>
    <h5><a href="single.html"><%= product.category %></a></h5>
    <div class="simpleCart_shelfItem">
      <p><i class="item_price"></i><%= t("layouts.product.related.record.priceunit", price: product.price) %></p>
      <p><a class="item_add" href="#"><%= t("layouts.product.related.record.addcart") %></a></p>
    </div>

    <!-- New Label
     <div class="dresses_grid_pos">
      <h6>New</h6>
    </div> -->
  </div>
